{
  "name": "@playkit-js/qna",
  "version": "2.1.5",
  "bugs": {
    "url": "https://github.com/kaltura/playkit-js-qna/issues"
  },
  "homepage": "https://github.com/kaltura/playkit-js-qna#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaltura/playkit-js-qna.git"
  },
  "description": "",
  "main": "dist/playkit-qna.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "watch": "webpack --progress --color --watch --mode development",
    "test": "jest",
    "release": "standard-version",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin master",
    "eslint": "eslint . --color",
    "commit:dist": "git add --force --all dist && (git commit -m 'chore: update dist' || exit 0)",
    "docs:generate": "documentation build flow-typed/** src/** -f md -o docs/configuration.md",
    "docs:serve": "documentation serve flow-typed/** src/** --watch",
    "precommit": "lint-staged",
    "prettier:fix": "prettier --write ."
  },
  "license": "AGPL-3.0",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src"
  ],
  "author": "",
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "conventional-github-releaser": "3.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "documentation": "^13.2.5",
    "eslint": "^6.6.0",
    "jest": "^28.1.0",
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.3",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "1.3.0",
    "ts-loader": "^9.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "@playkit-js-contrib/common": "^4.1.10",
    "@playkit-js-contrib/linkify": "^4.1.3",
    "@playkit-js-contrib/ui": "^4.1.13",
    "classnames": "^2.2.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run --silent lint:fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "qna",
    "kaltura",
    "player",
    "playkit-js",
    "playkit-js-qna",
    "html5 player"
  ]
}
